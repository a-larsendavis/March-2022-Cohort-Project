    <h1>Neighbor Gather</h1>

    <div class="loginform">
      <form id="login" class="row g-3">
        <fieldset>
          <legend>
            <i class="fa-solid fa-house-chimney-window"></i>Log In
          </legend>
          <p>Username</p>
          <input
            type="text"
            id="username"
            name="username"
            placeholder="Username"
            required
          />
          <br />
          <p>Password</p>
          <input
            type="password"
            id="password"
            name="password"
            placeholder="Password"
            required
          />
          <br />
          <button>Submit</button>
        </fieldset>
      </form>
    </div>

